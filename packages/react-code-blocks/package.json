{
  "name": "react-code-blocks",
  "version": "0.0.1",
  "description": "Modified Atlaskit's Code Block to support more languages (i.e graphql, reasonml, etc) and theme (i.e railscast, darcula, monokai, etc) code snippets!",
  "author": "rajinwonderland",
  "main": "build/index.js",
  "keywords": [
    "code-snippets",
    "code-blocks",
    "code-snippet-components",
    "react-syntax-highlighter",
    "mdx",
    "react-syntax-highlighting"
  ],
  "dependencies": {
    "@atlaskit/theme": "9.0.0",
    "copy-to-clipboard": "^3.2.0",
    "fbjs": "1.0.0",
    "hoist-non-react-statics": "3.3.0",
    "react-syntax-highlighter": "10.2.1",
    "react-tooltip": "^3.10.0",
    "styled-components": "4.2.0",
    "webpack": "^4.30.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "clean": "rm -rf build",
    "prettier": "prettier \"{src,test,examples}/**/*.js\" --write",
    "prepare": "yarn build",
    "prerelease": "yarn prettier && yarn test:coverage && yarn build",
    "release": "npm publish",
    "start": "webpack --watch",
    "build": "webpack",
    "size": "npx bundlesize"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "prettier": "^1.17.0",
    "webpack-cli": "^3.0.4"
  },
  "bundlesize": [
    {
      "path": "./build/index.js",
      "maxSize": "3 kB"
    }
  ]
}
